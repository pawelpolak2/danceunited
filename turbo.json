{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "generate": {
      "outputs": ["generated/**"]
    },
    "typegen": {
      "outputs": [".react-router/**"]
    },
    "lint": {
      "dependsOn": ["typegen", "^lint"]
    },
    "build": {
      "dependsOn": ["generate", "typegen", "^build"],
      "outputs": ["build/**", "dist/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
